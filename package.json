{
  "name": "angular2-compilation",
  "version": "1.0.0",
  "description": "Example of ahead-of-time compilation for Angular2",
  "main": "app.bundle.js",
  "scripts": {
    "start": "webpack-dev-server --quiet",
    "start:aot": "npm run ngc && npm run rollup && webpack-dev-server --quiet --compress --config webpack-aot.config.js",
    "ngc": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mike-plummer/angular2-compilation.git"
  },
  "author": "Mike Plummer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-plummer/angular2-compilation"
  },
  "homepage": "https://github.com/mike-plummer/angular2-compilation#readme",
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "core-js": "2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.8",
    "@angular/platform-server": "2.4.8",
    "@types/node": "^7.0.5",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.0.7",
    "css-loader": "0.26.2",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "pug": "2.0.0-beta11",
    "pug-html-loader": "1.1.0",
    "raw-loader": "0.5.1",
    "rollup": "0.41.4",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1",
    "style-loader": "0.13.2",
    "typescript": "2.1.5",
    "webpack": "2.2.1",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-server": "2.4.1"
  }
}
